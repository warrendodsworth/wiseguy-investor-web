{
  "name": "functions",
  "scripts": {
    "start": "tsc --project tsconfig.app.json --watch",
    "build": "tsc --project tsconfig.app.json",
    "deploy": "firebase use prod && firebase deploy --only functions",
    "test": "jest --config jest.config.js --verbose=true --passWithNoTests",
    "test_watch": "jest --config jest.config.js --watch --onlyChanged --passWithNoTests --detectOpenHandles --verbose=true",
    "lint": "npx eslint",
    "serve": "npm run build && firebase serve --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start_created_at_init": "npm run shell",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "20"
  },
  "main": "lib/index.js",
  "dependencies": {
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "firebase-admin": "^12.6.0",
    "firebase-functions": "^5.1.1",
    "luxon": "^3.4.4",
    "stripe": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/preset-env": "^7.25.8",
    "@eslint/js": "^9.7.0",
    "@types/axios": "^0.14.0",
    "@types/busboy": "^1.5.4",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.5",
    "@types/luxon": "^3.4.2",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "babel-jest": "^29.7.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.7.0",
    "eslint-plugin-import": "^2.29.1",
    "firebase-functions-test": "^3.3.0",
    "globals": "^15.8.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.3",
    "typescript": "4.7.4",
    "typescript-eslint": "^7.16.1"
  },
  "private": true,
  "overrides": {
    "eslint": "^9.7.0"
  }
}
